class Solution:
    def search(self, nums: List[int], target: int) -> int:
        left = -1
        right = len(nums)
        while left < right - 1:
            m = left + (right - left) // 2
            if nums[m] <= target:
                left = m
            else:
                right = m
        return left if left >= 0 and left < len(nums) and nums[left] == target else -1
